{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm ci",
  "env": {
    "NEXT_PUBLIC_FEATURE_AUTH": "false",
    "NEXT_PUBLIC_FEATURE_SUPABASE_SYNC": "false",
    "NEXT_PUBLIC_FEATURE_SESSION_EXPORT": "false"
  },
  "git": {
    "deploymentEnabled": {
      "dev": false
    }
  }
}
